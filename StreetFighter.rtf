        blackKick.centerX ==10\
        \
    if('up' in keys):\
        blackGuyMove.visible = True\
        blackGuyPunch.visible = False\
        \
    if('down' in keys):\
        blackTop.visible = False\
        blackKick.visible = False        \
        blackGuyMove.visible = True\
    \
    if('a' in keys):\
        whiteGuyMove.centerX ==10\
        whiteGuyPunch.centerX ==10\
