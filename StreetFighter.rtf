def onKeyPress(key):\
    \
    if(P2.visible == True and P1.visible == True):\
        if(whiteGuyMove.right <=blackGuyMove.left - whiteGuyMove.right+ 167):\
            if(blackGuyMove.visible ==True):\
            \
                if(key == 'right' and blackGuyMove.centerX <=370):\
                    blackGuyMove.centerX +=10\
                    blackGuyPunch.centerX+=10\
                    blackKick.centerX+=10\
                    blackTop.centerX+=10\
\
                if(key == 'left'):\
                    blackGuyMove.centerX -=10\
                    blackGuyPunch.centerX -=10\
                    blackKick.centerX -=10\
                    blackTop.centerX-=10\
                    \
\
            if(key == 'up'):\
                blackGuyMove.visible = False\
                blackGuyPunch.visible = True\
                blackGuyPunch.toFront()\
                if(blackPunch.hitsShape(whiteGuyMove or whiteGuyPunch or whiteKick)):\
                    P1hit()\
                    \
                    \
            if(key == 'down'):\
                blackGuyMove.visible = False\
                blackTop.visible = True\
                blackKick.visible = True\
                blackKick.toFront()\
                if(blackKick.hitsShape(whiteGuyMove or whiteGuyPunch or whiteKick)):\
                    P1hit()\
\
            if(whiteGuyMove.visible == True):    \
                if(key == 'a' and whiteGuyMove.centerX >=30):\
                    whiteGuyMove.centerX -=10\
                    whiteGuyPunch.centerX -=10\
                    whiteKick.centerX -=10\
                    whiteTop.centerX-=10\
\
                if(key == 'd'):\
                    whiteGuyMove.centerX +=10\
                    whiteGuyPunch.centerX +=10\
                    whiteKick.centerX +=10\
                    whiteTop.centerX+=10\
\
            if(key == 'w'):\
                whiteGuyMove.visible = False\
                whiteGuyPunch.visible = True\
                whiteGuyPunch.toFront()\
            \
                if(whitePunch.hitsShape(blackGuyMove or blackGuyPunch or blackKick)):\
                    P2hit()\
            if(key == 's'):\
                whiteGuyMove.visible = False\
                whiteTop.visible = True\
                whiteKick.visible = True\
                whiteKick.toFront()\
            \
                if(whiteKick.hitsShape(blackGuyMove or blackGuyPunch or blackKick)):\
                    P2hit()\
        else:\
            whiteGuyMove.centerX -=1\
            whiteGuyPunch.centerX -=1\
            whiteKick.centerX -=1\
            whiteTop.centerX -=1\
    \
    else:\
        if(P1.visible ==False):\
            header.value = 'Player 2 Wins!'\
        \
        if(P2.visible == False):\
            header.value = 'Player 1 Wins!'\
